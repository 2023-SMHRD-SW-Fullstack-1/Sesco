<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.smhrd.sesco.mapper.HtSurveyMapper">

	


	<insert id="insertSurvey" parameterType="com.smhrd.sesco.domain.Survey">
		<selectKey keyProperty="sv_seq" resultType="String" order="AFTER">
			SELECT LAST_INSERT_ID()
		</selectKey>
		INSERT INTO t_survey (sv_dt, hsv_seq, kid_seq)
		VALUES (#{sv_dt}, #{hsv_seq}, #{kid_seq})
	</insert>
	
</mapper>